import java.util.Scanner;

public class SuperString 
{

public static void main(String[] args) 
{

Scanner sc=new Scanner(System.in);
int t=sc.nextInt();
while(t-->0)
{
	String s=sc.next();
	int a[]=new int[26];
	for(int i=0;i<s.length();i++)
	{
		a[s.charAt(i)-97]++;
	}
	for(int i=0;i<s.length();i++)
	{
		if(a[s.charAt(i)-96]!=(s.charAt(i)-96))
		{
		System.out.println("no");
		
		}
		
		
	}
	
		
	
}
sc.close();

}
}
